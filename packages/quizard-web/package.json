{
    "name": "quizard-web",
    "version": "1.0.0",
    "scripts": {
        "tsc": "tsc -p ./tsconfig.json",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "prebuild": "npm run build-env",
        "build": "vite build",
        "build-gql": "graphql-codegen -c ./scripts/graphql-codegen.ts",
        "build-gql:watch": "npm run build-gql -- --watch",
        "build-env": "ts-node -P ../../tsconfig.node.json ./scripts/buildEnv.ts",
        "predev": "npm run build-env && npm run build-gql",
        "dev": "vite --port 8000"
    },
    "devDependencies": {
        "@parcel/watcher": "2.3.0",
        "@types/lodash": "4.14.202",
        "@types/react": "18.2.45",
        "@types/react-dom": "18.2.18",
        "@vitejs/plugin-react-swc": "3.5.0",
        "git-branch": "2.0.1",
        "vite": "5.0.10"
    },
    "dependencies": {
        "@apollo/client": "3.8.8",
        "@aws-amplify/ui-react": "6.0.7",
        "@graphql-codegen/cli": "5.0.0",
        "@graphql-codegen/client-preset": "4.1.0",
        "aws-amplify": "6.0.9",
        "graphql": "16.8.1",
        "lodash": "4.17.21",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-icons": "4.12.0",
        "react-router": "6.21.0",
        "react-router-dom": "6.21.0",
        "styled-components": "6.1.3",
        "zustand": "4.4.7"
    }
}
